<div style="margin-top: 35px;">
  <div class="container-index">
    <div class="email-index" style="position: relative;">
      <% @emails.each do |email| %>
        <div class="email-card link d-flex justify-content-between">
          <h4 style="width: 50%">Email title: <%= email.title %></h4>
          <h5 style="width: 30%">Sent date: <strong><%= email.consultation.end_time.strftime("%H:%M") %> <%= email.consultation.end_time.strftime("%m/%d/%Y") %></strong></h5>
          <h5 style="width: 20%"><%= email.consultation.patient.first_name%> <%= email.consultation.patient.last_name %></h5>
        </div>
      <% end %>
    </div>
  </div>
</div>

